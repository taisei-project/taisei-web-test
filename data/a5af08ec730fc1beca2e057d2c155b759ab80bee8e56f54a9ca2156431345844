#version 300 es
precision mediump float;
precision highp int;

uniform highp sampler2D tex;

layout(location = 0) out highp vec4 fragColor;
in highp vec2 texCoord;

void main()
{
    highp vec4 _83 = texture(tex, texCoord);
    highp float _87 = _83.w;
    highp vec4 _99 = vec4((vec3(1.0) - (_83.xyz / vec3(max(0.00999999977648258209228515625, _87)))) * _87, 0.0);
    if (all(lessThan(_99, vec4(1.525902189314365386962890625e-05))))
    {
        discard;
    }
    fragColor = _99;
}

